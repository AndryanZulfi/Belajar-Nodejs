<div class="section">
    <h2>Update Student</h2>
    <form id="updateStudentForm" action="/update" method="post">
        <label for="updateId">Student ID:</label>
        <input type="number" id="updateId" name="updateId" required>
        
        <label for="updateName">Name:</label>
        <input type="text" id="updateName" name="updateName">
        
        <label for="updateEmail">Email:</label>
        <input type="email" id="updateEmail" name="updateEmail">
        
        <label for="updateAge">Age:</label>
        <input type="number" id="updateAge" name="updateAge">
        
        <button type="submit">Update Student</button>
    </form>
</div>